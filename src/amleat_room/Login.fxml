<AnchorPane id="loginLiyout" prefHeight="216.0" prefWidth="401.0" stylesheets="@Style_Logein.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="amleat_room.LoginController">
   <children>
      <Label layoutX="233.0" layoutY="37.0" prefHeight="17.0" prefWidth="137.0" text="اسم المستخدم :" textFill="WHITE">
         <font>
            <Font name="Air Strip Arabic" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="74.0" layoutY="38.0" nodeOrientation="RIGHT_TO_LEFT" />
      <Label layoutX="234.0" layoutY="71.0" prefHeight="17.0" prefWidth="137.0" text="كلمة المرور :" textFill="WHITE">
         <font>
            <Font name="Air Strip Arabic" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="password" layoutX="74.0" layoutY="72.0" nodeOrientation="RIGHT_TO_LEFT" onAction="#login" />
      <Button layoutX="210.0" layoutY="130.0" mnemonicParsing="false" onAction="#login" onMouseEntered="#entred" onMouseExited="#exited" prefHeight="37.0" prefWidth="112.0" text="دخول">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="80.0" layoutY="130.0" mnemonicParsing="false" onAction="#exit" onMouseEntered="#entred" onMouseExited="#exited" prefHeight="37.0" prefWidth="112.0" text="خروج">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Line endX="146.0" layoutX="240.0" layoutY="114.0" startX="-225.0" />
   </children>
</AnchorPane>
